{"ast":null,"code":"import ShoppingCart from './ShoppingCart.vue';\nexport default {\n  components: {\n    ShoppingCart\n  },\n  name: 'NavHeader',\n  computed: {\n    userEmail() {\n      return this.isLoggedIn ? this.currentUser.email : '';\n    },\n    isAuthenticated() {\n      console.log(this.$store.state.user.isAuthenticated);\n      return this.$store.state.user.isAuthenticated;\n    }\n  },\n  methods: {\n    onLoginClicked() {\n      window.location = this.$store.state.endpoints.login;\n    },\n    onLogoutClicked() {\n      this.$store.commit(\"logout\");\n    },\n    getUserName() {\n      return this.$store.state.user.name;\n    }\n  }\n};","map":{"version":3,"names":["ShoppingCart","components","name","computed","userEmail","isLoggedIn","currentUser","email","isAuthenticated","console","log","$store","state","user","methods","onLoginClicked","window","location","endpoints","login","onLogoutClicked","commit","getUserName"],"sources":["/Users/nanhomkham/IdeaProjects/web-shop/src/components/views/NavHeader.vue"],"sourcesContent":["<template>\n  <nav class=\"navbar navbar-expand-sm navbar-dark bg-dark\" role=\"navigation\">\n    <div class=\"container\">\n      <router-link to=\"/\" class=\"navbar-brand mr-auto\">Blue Online Store</router-link>\n      <ul class=\"navbar-nav mr-auto\">\n      </ul>\n      <ul class=\"nav navbar-nav\">\n        <router-link to=\"/\" tag=\"li\" v-if=\"!isAuthenticated\" class=\"nav-item\" active-class=\"active\">\n          <a @click=\"onLoginClicked\" class=\"nav-link\">Login</a>\n        </router-link>\n        <li v-if=\"isAuthenticated\" class=\"li-pointer nav-item\">\n          <div class=\"dropdown\">\n            <button class=\"btn btn-secondary dropdown-toggle\" type=\"button\" id=\"dropdownMenuButton\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">\n              {{ getUserName() }}\n            </button>\n            <div class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton\">\n              <a class=\"dropdown-item\" href=\"#\">Account Settings</a>\n              <a @click=\"onLogoutClicked\" class=\"dropdown-item\">Logout {{ userEmail }}</a>\n            </div>\n          </div>\n        </li>\n        <li>\n          <ShoppingCart />\n        </li>\n      </ul>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport ShoppingCart from './ShoppingCart.vue';\nexport default {\n  components: { ShoppingCart },\n  name: 'NavHeader',\n  computed: {\n    userEmail() {\n      return this.isLoggedIn ? this.currentUser.email : ''\n    },\n    isAuthenticated() {\n      console.log(this.$store.state.user.isAuthenticated)\n      return this.$store.state.user.isAuthenticated;\n    },\n  },\n  methods: {\n    onLoginClicked() {\n      window.location = this.$store.state.endpoints.login;\n    },\n    onLogoutClicked() {\n      this.$store.commit(\"logout\");\n    },\n    getUserName() {\n      return this.$store.state.user.name;\n    }\n  }\n}\n</script>"],"mappings":"AA8BA,OAAOA,YAAW,MAAO,oBAAoB;AAC7C,eAAe;EACbC,UAAU,EAAE;IAAED;EAAa,CAAC;EAC5BE,IAAI,EAAE,WAAW;EACjBC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACC,UAAS,GAAI,IAAI,CAACC,WAAW,CAACC,KAAI,GAAI,EAAC;IACrD,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChBC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACL,eAAe;MAClD,OAAO,IAAI,CAACG,MAAM,CAACC,KAAK,CAACC,IAAI,CAACL,eAAe;IAC/C;EACF,CAAC;EACDM,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACfC,MAAM,CAACC,QAAO,GAAI,IAAI,CAACN,MAAM,CAACC,KAAK,CAACM,SAAS,CAACC,KAAK;IACrD,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACT,MAAM,CAACU,MAAM,CAAC,QAAQ,CAAC;IAC9B,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACX,MAAM,CAACC,KAAK,CAACC,IAAI,CAACX,IAAI;IACpC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}