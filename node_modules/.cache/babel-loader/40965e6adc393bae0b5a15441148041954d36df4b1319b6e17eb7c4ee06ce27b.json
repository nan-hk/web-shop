ductsList',\n  data() {\n    return {\n      products: []\n    };\n  },\n  methods: {\n    fetchProductsList() {\n      fetch('https://fakestoreapi.com/products')\n          .then(res=>res.json())\n          .then(json=>{\n            this.products = json;\n          }).catch(error => {\n            return error\n          });\n    }\n  },\n  mounted() {\n    this.fetchProductsList();\n  }\n};\n</script>"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClBC,KAAK,CAAC,mCAAmC,EACpCC,IAAI,CAACC,GAAG,IAAEA,GAAG,CAACC,IAAI,CAAC,CAAC,EACpBF,IAAI,CAACE,IAAI,IAAE;QACV,IAAI,CAACN,QAAO,GAAIM,IAAI;MACtB,CAAC,CAAC,CAACC,KAAK,CAACC,KAAI,IAAK;QAChB,OAAOA,KAAI;MACb,CAAC,CAAC;IACR;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,iBAAiB,CAAC,CAAC;EAC1B;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}